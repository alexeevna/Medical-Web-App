{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "environment": [
        {
          "name": "SERVER_PORT",
          "value": "7999"
        },
        {
          "name": "MIRF_ORCHESTRATOR_URL",
          "value": "mirforchestrator"
        },
        {
          "name": "MIRF_ORCHESTRATOR_PORT",
          "value": "5011"
        },
        {
          "name": "MIRF_REPOSITORY_URL",
          "value": "mirfrepository"
        },
        {
          "name": "MIRF_REPOSITORY_PORT",
          "value": "8050"
        },
        {
          "name": "ORTHANC_URL",
          "value": "http://orthanc:8042"
        },
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://medwebpostgres:5555/medwebappdb"
        },
        {
          "name": "SFTP_URL",
          "value": "sftp"
        },
        {
          "name": "SFTP_PORT",
          "value": "22"
        }
      ],
      "essential": true,
      "image": "ashvyrkova/medwebapp",
      "name": "medwebapp",
      "memory": 700,
      "links": [
        "orthanc",
        "medwebpostgres",
        "sftp",
        "mirforchestrator",
        "mirfrepository"
      ],
      "command": [
        "sleep",
        "10s"
      ],
      "portMappings": [
        {
          "containerPort": 7999,
          "hostPort": 80
        }
      ]
    },
    {
      "environment": [
        {
          "name": "POSTGRES_PASSWORD",
          "value": "secretpassword"
        },
        {
          "name": "POSTGRES_DB",
          "value": "medwebappdb"
        },
        {
          "name": "POSTGRES_PORT",
          "value": "5555"
        },
        {
          "name": "PGPORT",
          "value": "5555"
        }
      ],
      "essential": true,
      "image": "postgres",
      "name": "medwebpostgres",
      "memory": 600,
      "portMappings": [
        {
          "containerPort": 5555,
          "hostPort": 5555
        }
      ]
    },
    {
      "command": [
        "sh",
        "wait-for-orchestrator.sh",
        "java",
        "-jar",
        "repository-1.0-SNAPSHOT.jar"
      ],
      "environment": [
        {
          "name": "SERVER_PORT",
          "value": "8050"
        },
        {
          "name": "ORCHESTRATOR_HOST",
          "value": "mirforchestrator"
        },
        {
          "name": "ORCHESTRATOR_PORT",
          "value": "5011"
        }
      ],
      "links": [
        "mirforchestrator"
      ],
      "essential": false,
      "image": "ashvyrkova/mirfrepository",
      "name": "mirfrepository",
      "memory": 400,
      "portMappings": [
        {
          "containerPort": 8050,
          "hostPort": 8050
        }
      ]
    },
    {
      "environment": [
        {
          "name": "SERVER_PORT",
          "value": "5011"
        },
        {
          "name": "MEDICAL_WEB_APP_URL",
          "value": "medwebapp.us-east-1.elasticbeanstalk.com"
        }
      ],
      "essential": false,
      "image": "ashvyrkova/mirforchestrator",
      "name": "mirforchestrator",
      "memory": 500,
      "portMappings": [
        {
          "containerPort": 5011,
          "hostPort": 5011
        }
      ]
    },
    {
      "essential": true,
      "image": "jodogne/orthanc",
      "name": "orthanc",
      "memory": 500,
      "portMappings": [
        {
          "containerPort": 8042,
          "hostPort": 8042
        }
      ]
    },
    {
      "command": [
        "sh",
        "wait-for-orchestrator.sh",
        "java",
        "-jar",
        "block-1.0-SNAPSHOT.jar"
      ],
      "environment": [
        {
          "name": "SERVER_PORT",
          "value": "8063"
        },
        {
          "name": "BLOCK_TYPE",
          "value": "pipelineForDeveloping"
        },
        {
          "name": "ORCHESTRATOR_HOST",
          "value": "mirforchestrator"
        },
        {
          "name": "ORCHESTRATOR_PORT",
          "value": "5011"
        }
      ],
      "links": [
        "mirforchestrator"
      ],
      "essential": false,
      "image": "ashvyrkova/mirfblock",
      "memory": 600,
      "name": "pipelinealg"
    },
    {
      "command": [
        "medwebuser:secretpassword:::upload"
      ],
      "essential": true,
      "image": "atmoz/sftp",
      "name": "sftp",
      "memory": 500,
      "portMappings": [
        {
          "containerPort": 22,
          "hostPort": 4999
        }
      ]
    }
  ],
  "family": "",
  "volumes": []
}
